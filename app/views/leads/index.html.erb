<p id="notice"><%= notice %></p>
<h1>Leads</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th>Website url</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Website goals</th>
      <th>Contacted</th>
    </tr>
  </thead>

  <tbody>
    <%= link_to 'New Lead', new_lead_path %>
    <hr />
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.name %></td>
        <td><%= lead.company %></td>
        <td><%= lead.website_url %></td>
        <td><%= lead.phone %></td>
        <td><%= lead.email %></td>
        <td><%= lead.address %></td>
        <td><%= lead.city %></td>
        <td><%= lead.state %></td>
        <td><%= lead.zip %></td>
        <td><%= lead.website_goals %></td>
        <td><%= lead.contacted %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
